<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with type="com.lacazuela.app.client.LaCazuelaResources" field="resource" />
	
	<g:SimplePanel>
		<g:HorizontalPanel>
			<g:Image ui:field="image"></g:Image>
			<g:Image resource="{resource.noimage}"></g:Image>
			<g:HTML ui:field="html"></g:HTML>
		</g:HorizontalPanel>
	</g:SimplePanel>
	
</ui:UiBinder> 